{
  "name": "lunews",
  "version": "0.0.1",
  "description": "Lunews, Usenet client",
  "main": "./dist/Server.js",
  "author": "Adam Kvasnicka",
  "license": "MIT",
  "repository": "https://github.com/NomiAdam/lunews",
  "private": false,
  "scripts": {
    "start": "node ./dist/Server.js",
    "server": "nodemon ./dist/Server.js --ignore tests",
    "client": "node ./client/scripts/start.js --prefix client",
    "install:server": "npm install",
    "install:client": "npm run --prefix ./client install:prod",
    "build:docker": "node ./dist/DockerSetup.js",
    "build:server": "tsc --skipLibCheck",
    "build:client": "npm run --prefix ./client build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "test": "jest ./dist",
    "test:watch": "jest --watch ./dist"
  },
  "dependencies": {
    "async-busboy": "0.7.0",
    "bcrypt-nodejs": "0.0.3",
    "chalk": "2.4.2",
    "clear": "0.1.0",
    "commander": "2.20.0",
    "concurrently": "4.1.0",
    "dotenv": "6.2.0",
    "emailjs-mime-codec": "2.0.8",
    "emailjs-mime-parser": "2.0.7",
    "entepe": "0.0.4",
    "inquirer": "6.3.1",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-logger": "3.2.0",
    "koa-passport": "4.1.3",
    "koa-router": "7.4.0",
    "koa-session": "5.10.1",
    "koa-static": "5.0.0",
    "mailparser": "2.6.0",
    "nodemailer": "5.1.1",
    "passport-google-oauth20": "1.0.0",
    "ramda": "0.26.1",
    "sqlite3": "git+https://github.com/NomiAdam/node-sqlite3",
    "typescript": "3.4.4"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/inquirer": "0.0.42",
    "@types/jest": "22.2.3",
    "@types/jsonwebtoken": "7.2.8",
    "@types/koa": "2.0.48",
    "@types/koa-bodyparser": "5.0.2",
    "@types/koa-logger": "3.1.1",
    "@types/koa-passport": "4.0.2",
    "@types/koa-router": "7.0.40",
    "@types/koa-session": "5.10.0",
    "@types/koa-static": "4.0.1",
    "@types/mailparser": "2.4.0",
    "@types/passport": "0.4.7",
    "@types/passport-http-bearer": "1.0.33",
    "@types/ramda": "0.25.51",
    "@types/sqlite3": "3.1.5",
    "@types/supertest": "2.0.7",
    "api-benchmark": "0.5.0",
    "jest": "22.4.4",
    "nodemon": "1.18.11",
    "supertest": "3.4.2"
  },
  "preferGlobal": true,
  "bin": {
    "nntp": "./dist/setup/Setup.js"
  }
}
